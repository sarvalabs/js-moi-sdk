{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "clean": {
            "cache": false
        },
        "clean:node_modules": {
            "cache": false
        },
        "check-types": {
            "cache": false
        },
        "build:esm": {
            "cache": false
        },
        "build:cjs": {
            "cache": false
        },
        "build": {
            "cache": true,
            "outputs": [
                "lib.cjs",
                "lib.esm"
            ]
        }
    }
}