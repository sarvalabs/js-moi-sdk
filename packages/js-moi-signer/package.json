{
  "name": "js-moi-signer",
  "version": "0.6.0",
  "description": "This package enables users to sign and verify messages in their applications.",
  "main": "./lib.cjs/index.js",
  "module": "./lib.esm/index.js",
  "scripts": {
    "build:cjs": "tsc --outDir ./lib.cjs",
    "watch:cjs": "tsc --outDir ./lib.cjs --watch",
    "build:esm": "tsc --outDir ./lib.esm --module ESNext --moduleResolution Bundler",
    "watch:esm": "tsc --outDir ./lib.esm --module ESNext --moduleResolution Bundler --watch",
    "watch": "npm run watch:cjs & npm run watch:esm",
    "build": "npm run build:cjs && npm run build:esm",
    "check-types": "tsc --noEmit",
    "clean": "rm -rf ./lib.cjs ./lib.esm",
    "clean:node_modules": "rm -rf node_modules"
  },
  "homepage": "https://docs.moi.technology/docs/build/packages/js-moi-sdk",
  "repository": {
    "type": "git",
    "url": "git://github.com/sarvalabs/js-moi-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/sarvalabs/js-moi-sdk/issues"
  },
  "keywords": [
    "moichain",
    "moi",
    "signer"
  ],
  "author": "Sarva Labs Inc. & MOI Protocol Developers",
  "license": "Apache-2.0 OR MIT",
  "dependencies": {
    "js-moi-providers": "^0.6.0",
    "js-moi-utils": "^0.6.0",
    "@noble/hashes": "^1.1.5",
    "@noble/secp256k1": "^1.7.1"
  }
}